
.allLimit {
	width: 75%;
	margin: 0 auto;
}
.header {
	width: 100%;
	height: 50vw;
	background-color: #fff;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.headerBox {
	width: 30vw;
	height: 30vw;
	-webkit-box-shadow: 5px 5px 28px 11px rgba(200, 200, 200, 0.2);
	        box-shadow: 5px 5px 28px 11px rgba(200, 200, 200, 0.2);
	margin-top: 5vw;
	-webkit-border-radius: 4vw;
	        border-radius: 4vw;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.headerBox image {
	width: 15vw;
	height: 15vw;
}
.content {
	width: 100%;
}
.contentTitle {
	font-size: 7vw;
	font-weight: bold;
}
.loginText {
	display: block;
	text-align: center;
	color: #9f9f9f;
}
.content input {
	width: 100%;
	height: 15vw;
	background-color: #f7f7f7;
	margin: 5vw 0;
	text-indent: 10vw;
	-webkit-border-radius: 7.5vw;
	        border-radius: 7.5vw;
}
.bottom {
	width: 100%;
	height: 35vw;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.bottom button {
	margin: 0 auto;
	width: 70%;
	height: 15vw;
	background-color: #fbbd08;
	-webkit-border-radius: 7.5vw;
	        border-radius: 7.5vw;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	color: #fff;
	font-weight: bold;
	-webkit-box-shadow: 9px 5px 28px 3px rgba(255, 190, 0, 0.46);
	        box-shadow: 9px 5px 28px 3px rgba(255, 190, 0, 0.46);
}

